{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always", { "omitLastInOneLineBlock": true }],
        "no-cond-assign": 0,
        "no-constant-condition": 0
    }
}
